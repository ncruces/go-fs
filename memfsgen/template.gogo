package {{.Package}}

import "time"
import "github.com/ncruces/go-fs/memfs"

var {{.Variable}} = memfs.Create()

func init() {
	var fs = {{.Variable}}
	{{- range .Assets}}
	fs.CreateString({{printf "%#v" .Name}}, {{printf "%#v" .Type}}, time.Unix({{.Time}}, 0), ""
		{{- range .Lines}}+
		"{{.}}"
    {{- end}})
	{{- end}}
}
